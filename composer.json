{
  "name": "tourze/aws-s3-storage-bundle",
  "description": "Symfony bundle for AWS S3 file storage using async-aws/s3",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "async-aws/s3": "^2.1",
    "doctrine/doctrine-bundle": "^2.13",
    "league/flysystem": "^3.10",
    "league/flysystem-async-aws-s3": "^3.10",
    "monolog/monolog": "^3.1",
    "psr/log": "^3|^2|^1",
    "symfony/config": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/doctrine-bridge": "^7.3",
    "symfony/event-dispatcher": "^7.3",
    "symfony/framework-bundle": "^7.3",
    "symfony/http-foundation": "^7.3",
    "symfony/http-kernel": "^7.3",
    "symfony/property-access": "^7.3",
    "symfony/yaml": "^7.3",
    "tourze/bundle-dependency": "1.*",
    "tourze/file-storage-bundle": "0.0.*",
    "tourze/symfony-dependency-service-loader": "1.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "symfony/http-client-contracts": "^3.6",
    "tourze/phpunit-base": "1.*",
    "tourze/phpunit-symfony-kernel-test": "1.0.*",
    "tourze/phpunit-symfony-unit-test": "1.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\AwsS3StorageBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\AwsS3StorageBundle\\Tests\\": "tests"
    }
  }
}